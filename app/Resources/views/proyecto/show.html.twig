{% extends 'base.html.twig' %}

{% block body %}
    <h1>Proyecto</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proyecto.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ proyecto.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ proyecto.descripcion }}</td>
            </tr>
            <tr>
                <th>Ubicacion</th>
                <td>{{ proyecto.ubicacion }}</td>
            </tr>
            <tr>
                <th>Planoubicacion</th>
                <td>{{ proyecto.planoUbicacion }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{% if proyecto.estado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{% if proyecto.fechaCreacion %}{{ proyecto.fechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Presupuestoinicial</th>
                <td>{{ proyecto.presupuestoInicial }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{% if proyecto.fechaInicio %}{{ proyecto.fechaInicio|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{% if proyecto.fechaFin %}{{ proyecto.fechaFin|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Prorroga1</th>
                <td>{% if proyecto.prorroga1 %}{{ proyecto.prorroga1|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Prorroga2</th>
                <td>{% if proyecto.prorroga2 %}{{ proyecto.prorroga2|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Prorroga3</th>
                <td>{% if proyecto.prorroga3 %}{{ proyecto.prorroga3|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('proyecto_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('proyecto_edit', { 'id': proyecto.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
